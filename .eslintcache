\";\nimport { Marginer } from \"../marginer\";\nimport {\n  BoldLink,\n  BoxContainer,\n  FieldContainer,\n  FieldError,\n  FormContainer,\n  FormError,\n  Input,\n  MutedLink,\n  SubmitButton,\n} from \"./common\";\nimport { AccountContext } from \"./context\";\nimport * as yup from \"yup\";\nimport axios from \"axios\";\n\nconst validationSchema = yup.object({\n  email: yup.string().required(),\n  password: yup.string().required(),\n});\n\nexport function LoginForm(props) {\n  const { switchToSignup } = useContext(AccountContext);\n  const [error, setError] = useState(null);\n\n  const onSubmit = async (values) => {\n    setError(null);\n    const response = await axios\n      .post(\"http://localhost:5000/api/v1/login\", values)\n      .catch((err) => {\n        if (err && err.response) setError(err.response.data.message);\n      });\n\n    if (response) {\n      alert(\"Welcome back in. Authenticating...\");\n    }\n  };\n\n  const formik = useFormik({\n    initialValues: { email: \"\", password: \"\" },\n    validateOnBlur: true,\n    onSubmit,\n    validationSchema: validationSchema,\n  });\n\n  return (\n    <BoxContainer>\n      <FormError>{error ? error : \"\"}</FormError>\n      <FormContainer onSubmit={formik.handleSubmit}>\n        <FieldContainer>\n          <Input\n            name=\"email\"\n            placeholder=\"Email\"\n            value={formik.values.email}\n            onChange={formik.handleChange}\n            onBlur={formik.handleBlur}\n          />\n          {\n            <FieldError>\n              {formik.touched.email && formik.errors.email\n                ? formik.errors.email\n                : \"\"}\n            </FieldError>\n          }\n        </FieldContainer>\n        <FieldContainer>\n          <Input\n            name=\"password\"\n            type=\"password\"\n            placeholder=\"Password\"\n            value={formik.values.password}\n            onChange={formik.handleChange}\n            onBlur={formik.handleBlur}\n          />\n          {\n            <FieldError>\n              {formik.touched.password && formik.errors.password\n                ? formik.errors.password\n                : \"\"}\n            </FieldError>\n          }\n        </FieldContainer>\n        <MutedLink href=\"#\">Forgot Password?</MutedLink>\n        <Marginer direction=\"vertical\" margin=\"1em\" />\n        <SubmitButton type=\"submit\" disabled={!formik.isValid}>\n          Login\n        </SubmitButton>\n      </FormContainer>\n      <Marginer direction=\"vertical\" margin={5} />\n      <MutedLink href=\"#\">\n        Dont have an Account?\n        <BoldLink href=\"#\" onClick={switchToSignup}>\n          sign up\n        </BoldLink>\n      </MutedLink>\n    </BoxContainer>\n  );\n}\n",["55","56"],"/home/islempenywis/Source/Tutos/login-jwt-demo/src/reportWebVitals.js",[],"/home/islempenywis/Source/Tutos/login-jwt-demo/src/components/marginer/index.jsx",[],{"ruleId":"57","severity":1,"message":"58","line":1,"column":17,"nodeType":"59","messageId":"60","endLine":1,"endColumn":30},{"ruleId":"57","severity":1,"message":"61","line":7,"column":10,"nodeType":"59","messageId":"60","endLine":7,"endColumn":19},{"ruleId":"57","severity":1,"message":"62","line":1,"column":10,"nodeType":"59","messageId":"60","endLine":1,"endColumn":15},{"ruleId":"57","severity":1,"message":"62","line":1,"column":10,"nodeType":"59","messageId":"60","endLine":1,"endColumn":15},{"ruleId":"57","severity":1,"message":"63","line":1,"column":8,"nodeType":"59","messageId":"60","endLine":1,"endColumn":12},{"ruleId":"57","severity":1,"message":"62","line":1,"column":10,"nodeType":"59","messageId":"60","endLine":1,"endColumn":15},{"ruleId":"64","replacedBy":"65"},{"ruleId":"66","replacedBy":"67"},"no-unused-vars","'createContext' is defined but never used.","Identifier","unusedVar","'MutedLink' is defined but never used.","'Field' is defined but never used.","'logo' is defined but never used.","no-native-reassign",["68"],"no-nega